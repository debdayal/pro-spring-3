<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd"
    default-lazy-init="true">
    
    <context:annotation-config/>
    
    <bean id="simpleBean1" class="com.apress.prospring3.ch5.lifecycle.SimpleBeanWithJSR250" init-method="myInit">
        <property name="name">
            <value>Clarence Ho</value>
        </property>
        <property name="age">
            <value>100</value>
        </property>
    </bean>
    
    <bean id="simpleBean2" class="com.apress.prospring3.ch5.lifecycle.SimpleBeanWithJSR250">
        <property name="age">
            <value>100</value>
        </property>
    </bean>
    
    <bean id="simpleBean3" class="com.apress.prospring3.ch5.lifecycle.SimpleBeanWithJSR250">
        <property name="name">
            <value>Clarence Ho</value>
        </property>
    </bean>
</beans>